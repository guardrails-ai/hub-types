{
  "name": "hub-types",
  "version": "0.0.0",
  "description": "Library composer for data structures used in the Guardrails Hub",
  "type": "module",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@hyperjump/json-schema": "^1.8.0",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "json-schema-faker": "^0.5.6",
    "json-schema-to-typescript": "^14.0.0",
    "lodash": "^4.17.21",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "ts-gen": "rm -rf ./build/ts && openapi-generator-cli generate --generator-key hub-ts",
    "openapi-gen": "rm -rf ./build/json-schema.json && rm -rf ./build/openapi-spec.json && node ./resources/gen-openapi-spec.js;",
    "py-gen": "rm -rf ./build/py && openapi-generator-cli generate --generator-key hub-py",
    "valiate-openapi-spec": "node ./resources/validate-openapi-spec.js",
    "valiate-json-schema": "node ./resources/validate-json-schema.js"
  },
  "author": "",
  "license": "ISC"
}
